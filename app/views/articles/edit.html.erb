<h1>Editing article</h1>
<%= form_for(@article) do |f| %>
<div class="card">
  <div class="card-body">
   <div class="mb-3">
  <%= f.label :title %>
  <%= f.text_field :title %>
   </div>

  <br>
   <div class="mb-3">
  <%= f.label :content %>
  <%= f.text_area :content %>
   </div>
  <%= f.submit class: "btn btn-primary"%>
    </div>
</div>
<% end %>
<%= link_to "back", articles_path %>
<%= link_to "show", article_path %>
